    "eslint-plugin-vue": "^10.5.1",
    "prettier": "^3.6.2",
    "stylelint-config-clean-order": "^7.0.0",
    "stylelint-config-standard": "^39.0.1",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.2",
